{
    "name": "apartment-manage",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/jest": "24.0.23",
        "@types/node": "12.12.14",
        "@types/react": "16.9.14",
        "@types/react-dom": "16.9.4",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-scripts": "3.2.0",
        "typescript": "3.7.2"
    },
    "scripts": {
        "build": "npm run prod-build",
        "build-copy": "react-scripts build && npm run copy-to-wwwroot",
        "only-build": "react-scripts build",
        "clean-server": "rimraf ./wwwroot",
        "copy-to-wwwroot": "npm run clean-server && ncp ./build ./wwwroot",
        "prod-build": "npm install && npm run-script build-copy"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "ncp": "^2.0.0",
        "rimraf": "^3.0.0"
    }
}